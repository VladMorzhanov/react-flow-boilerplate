{
  "name": "react-flow-boilerplate",
  "version": "2.1.0",
  "description": "React + Flow boilerplate written with Mobx, MST and styled-components.",
  "main": "./src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server  --config config/webpack.config.dev.js --mode development",
    "bld": "sh -ac '. ./.env.${REACT_APP_BUILD_MODE}; node ./config/scripts/build.js'",
    "build": "REACT_APP_BUILD_MODE=development yarn run bld",
    "staging": "REACT_APP_BUILD_MODE=staging yarn run bld",
    "production": "REACT_APP_BUILD_MODE=production yarn run bld",
    "flow start": "flow start",
    "flow stop": "flow stop",
    "flow status": "flow status",
    "flow coverage": "flow coverage"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
