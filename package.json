    "history": "^4.7.2",
    "mobx": "^4.3.0",
    "mobx-react": "^5.1.2",
    "mobx-react-router": "^4.0.4",
    "mobx-state-tree": "^2.0.5",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.0.0",
    "react-router": "^4.3.1",
    "reselect": "^3.0.1",
